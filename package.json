{
  "name": "graphql-object",
  "version": "1.0.0",
  "description": "Use GraphQL syntax to query into an object in-memory.",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "rollup src/index.js --file dist/index.js --format cjs",
    "build:esm": "rollup src/index.js --file dist/index.mjs --format esm"
  },
  "dependencies": {
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "rollup": "^1.27.10"
  },
  "keywords": [
    "graphql",
    "graphql-client",
    "graphql-tag"
  ]
}
